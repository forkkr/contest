const int mxn = 2;
typedef unsigned long long ull;
struct Matrix
{
	ull mat[mxn][mxn];
};
Matrix mul(Matrix a , Matrix b)
{
	Matrix ans;
	for(int i = 0; i < mxn; i++)
	{
		for(int j = 0; j < mxn; j++)
		{
			ans.mat[i][j] = 0;
			for(int k = 0; k < mxn; k++)
			{
				ans.mat[i][j] += a.mat[i][k]*b.mat[k][j];
			}
		}
	}
	return ans;
}
Matrix expo(Matrix base , int p)
{
	if(p==0)
	{
		Matrix ret;
		for(int i = 0; i < mxn; i++)
		{
			for(int j = 0; j < mxn; j++)
			{
				ret.mat[i][j] = (i==j);
			}
		}
		return ret;
	}
	Matrix x = expo(base , p/2);
