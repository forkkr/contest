int g[10005], pile[110];
 
int Mex( set<int> s)
{
    int mex = 0;
    while(s.find(mex)!=s.end())
        mex++;
    return mex;
}
 
int Grundy(int n)
{
    if(g[n]!=-1)
        return g[n];
    int mid = (n+1)/2;
    set<int > s;
    for(int i = 1; i < mid ; i++)
    {
        s.insert( Grundy(i)^Grundy(n-i));
    }
    g[n] = Mex(s);
    return g[n];
}

