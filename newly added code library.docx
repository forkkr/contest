************* Implicit Segment Tree********
typedef long long ll;
const ll mod = 1e9+7;
struct NODE
{
	ll sum;
	NODE* left , *right;
	NODE()
	{
		left = NULL;
		right = NULL;
		sum = 0;
	}
};

void update(NODE *node , ll beg , ll endd , ll pos , ll val)
{
	if(beg==pos&&endd==pos)
	{
		node->sum =val;
		return;
	}
	ll mid = (beg + endd)>>1;
	if(node->left==NULL)
	{
		node->left = new NODE();
	}
	if(node->right==NULL)
	{
		node->right = new NODE();
	}
	if(pos <= mid)
		update(node->left, beg , mid , pos , val);
	else
	{
		update(node->right, mid+1, endd , pos , val);
	}
	node->sum = node->left->sum + node->right->sum;
	return;
}
ll query(NODE* node , ll beg ,ll endd, ll l , ll r)
{
	if(node == NULL || l > r)
	return 0;
	if(l==beg && endd==r)
	return node->sum;
	ll mid = (beg+endd)>>1;
	ll r1 =query(node->left , beg, mid, l , min(mid,r));
	ll r2= query(node->right, mid+1, endd, max(l, mid+1), r);
	return r1+r2;
}


