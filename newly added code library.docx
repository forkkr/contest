const int mxn = 1e5;
int parent[mxn] , level[mxn];
typedef long long ll;
typedef pair< int , ll > pil;
typedef vector< pil > vpil;
pil sparse[mxn][22] ;
vpil adj[mxn];
ll mstCost , mstCount;
bool vis[mxn];
int n , m;
